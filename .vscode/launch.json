{
    "version": "0.2.0",
                        "configurations":
        [
              {
                "type": "node",
                "request": "launch",
                "name": "Launch unifiedServer",
                "runtimeExecutable": "npx",
                "runtimeArgs": ["ts-node"],
                "args": ["/workdir/dlmm-sdk/ts-client/src/server/unifiedServer.ts"],
                "cwd": "${workspaceFolder}",
                "sourceMaps": true,
                "console": "integratedTerminal"    
        }, 
        {
            "name": "Streamlit Debugger",
            "type": "debugpy",
            "request": "launch",
            "module": "streamlit",
            "console": "integratedTerminal",
            "args": [
                "run",
                "/workdir/dlmm-sdk/app.py",
                "--server.port", "8501"
            ]
        }
    ]
}
